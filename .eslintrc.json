{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["standard-with-typescript", "plugin:react/recommended", "prettier", "prettier/react"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react", "react-native"],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  }
  